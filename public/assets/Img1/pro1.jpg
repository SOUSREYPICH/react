<script>
// Please update variant id of gift box/wrap
  document.addEventListener('rebuy:smartcart.ready', () => {
    setTimeout(() => {
      if (document.querySelector('.rebuy-cart__flyout-note-toggle-input').checked && !Rebuy.Cart.cart.items.some(item => item.variant_id == 44428341194)) {
        Rebuy.libraries.$.post('/cart/add.js', {
          items: [{
            quantity: 1,
            id: 44428341194
          }]
        })
      }
  
      document.querySelector('.rebuy-cart__flyout-note-toggle-input').addEventListener('change', () => {
        setTimeout(() => {
          if (document.querySelector('.rebuy-cart__flyout-note-toggle-input').checked && !Rebuy.Cart.cart.items.some(item => item.variant_id == 44428341194)) {
            Rebuy.libraries.$.post('/cart/add.js', {
              items: [{
                quantity: 1,
                id: 44428341194
              }]
            })
          } else if (!document.querySelector('.rebuy-cart__flyout-note-toggle-input').checked && Rebuy.Cart.cart.items.some(item => item.variant_id == 44428341194)) {
            Rebuy.libraries.$.post('/cart/update.js', {
              updates: {
                44428341194: 0
              }
            })
          }
        }, 250)
      })
    }, 500)
  })

const orderNoteToggle = () => {
  const checkbox = document.querySelector('.rebuy-cart__flyout-note-toggle-input2');
  const textarea = document.querySelector('.rebuy-cart__flyout-order-note-textarea');
  const submitNoteButton = document.querySelector('.submit-note-button');

  checkbox.addEventListener('change', () => {
    textarea.classList.toggle('hidden', !checkbox.checked);
    submitNoteButton.classList.toggle('hidden', !checkbox.checked);
  });

  submitNoteButton.addEventListener('click', () => {
    textarea.classList.add('hidden');
    submitNoteButton.classList.add('hidden');
  });
};

  timer = null;

const updateCart = async (data = {}) => {
    const cartLocale = (window?.Shopify?.routes?.root || '/');
    const response = await fetch(cartLocale + 'cart/update.js', {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json'
        },
        body: JSON.stringify(data)
    });

    return response.json();
}

const updateCartAttributes = async (attributes) => {
    return await updateCart(
        {
            attributes: {
                ...Rebuy.Cart.cart.attributes,
                ...attributes
            }
        });
}

const initEventListeners = () => {

    // document.getElementById("rebuy-custom-name-form").addEventListener("submit", (e) => {
    //     e.preventDefault()
    //     document.getElementById("rebuy-custom-name-form").style.display = "none"
    //     alert("Response submitted!")
