    document.querySelector(".rebuy-cart__flyout-order-note-textarea").addEventListener('change', function () {

        const attributes = {
            'Order-Note': this.value
        };

        console.log('You have typed: ', attributes);

        updateCartAttributes(attributes);
    });
}


document.addEventListener('rebuy:smartcart.ready', (event) => {
    const orderNote = Rebuy?.Cart?.cart?.attributes['Order-Note'];
    
    console.log('Order-Note:', orderNote);

    if (orderNote) {
        document.querySelector(".rebuy-cart__flyout-order-note-textarea").value = sender;
    }

    initEventListeners();
});
</script>


